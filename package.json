{
  "name": "LOD",
  "version": "1.0.0",
  "description": "Flash Card Shark",
  "main": "lib/server.js",
  "author": "Max Bises",
  "license": "MIT",
  "scripts": {
    "dev-linux": "nodemon -r dotenv/config --exec babel-node lib/server.js",
    "webpack": "webpack -wd",
    "tags": "npx cucumber-js lib/cucumber/features --require-module @babel/register --require lib/Cucumber/steps --tags",
    "test:e2e": "npx cucumber-js lib/cucumber/features --require-module @babel/register --require lib/Cucumber/steps",
    "test:unit:coverage": "nyc --reporter=html --reporter=text yarn tags @AdminAPI"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "testEnvironment": "jsdom"
  },
  "babel": {
    "presets": [
      "@babel/react",
      "@babel/env"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-import-meta",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-json-strings",
      "@babel/plugin-transform-regenerator",
      "@babel/plugin-transform-runtime",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-proposal-function-sent",
      "@babel/plugin-proposal-export-namespace-from",
      "@babel/plugin-proposal-numeric-separator",
      "@babel/plugin-proposal-throw-expressions",
      "@babel/plugin-proposal-private-methods"
    ]
  },
  "devDependencies": {
    "@babel/node": "^7.0.0",
    "@babel/register": "^7.10.5",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-register": "^6.26.0",
    "chromedriver": "^84.0.1",
    "css-loader": "^3.2.1",
    "cucumber": "^6.0.5",
    "dotenv-cli": "^3.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-react": "^7.17.0",
    "identity-obj-proxy": "^3.0.0",
    "morgan": "^1.9.1",
    "nyc": "^15.1.0",
    "react-test-renderer": "^16.13.1",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "style-loader": "^1.0.1",
    "svg-inline-loader": "^0.8.0",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.8.0",
    "@babel/node": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-private-methods": "^7.7.4",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.8.0",
    "@babel/preset-env": "^7.8.0",
    "@babel/preset-react": "^7.7.4",
    "@reduxjs/toolkit": "^1.2.3",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "chalk": "^3.0.0",
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "file-loader": "^6.0.0",
    "gsap": "^3.2.6",
    "mime": "^2.4.4",
    "mysql": "^2.17.1",
    "nodemon": "^2.0.1",
    "react": "^16.12.0",
    "react-beforeunload": "^2.1.0",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dom": "^16.13.1",
    "react-keyboard-event-handler": "^1.5.4",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-tabs": "^3.0.0",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "uuid": "^8.3.0",
    "webpack": "^4.41.2"
  }
}
