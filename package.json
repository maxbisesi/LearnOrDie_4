    "nodemon": "^2.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-spring": "^8.0.27",
    "react-tabs": "^3.0.0",
    "redux": "^4.0.4",
    "webpack": "^4.41.2"
  }
